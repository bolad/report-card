  <div class="form-group hidden-field">
    <%= f.label :project_name, class: "head-small" %>
    <%= f.text_field :project_name, class: 'form-control' %>
  </div>
  <div class="form-group hidden-field">
    <%= f.label :project_number, class: "head-small" %>
    <%= f.text_field :project_number, class: 'form-control' %>
  </div>
  <div class="form-group hidden-field">
    <%= f.label :potential_impact, class: "head-small" %>
    <%= f.text_field :potential_impact, class: 'form-control' %>
