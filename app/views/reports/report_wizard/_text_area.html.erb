<%= form_for @report, url: report_report_wizard_path, method: :put do |f| %>
  <div class="field">
    <%= yield f %>
  </div>
  <div style="padding-top: 2rem;">
    <div class="btn btn-inverse">
      <%= f.submit "Save Changes" , style: "background-color: #262626; border: none; color: #fff; float: right;"%>
    </div>
  </div>
<% end %>