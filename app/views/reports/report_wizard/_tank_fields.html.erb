<div class="hidden-container nested-fields shadow rounded">
  <div class="form-group hidden-field">
    <label class="head-small">Tank Ref.</label>
    <%= f.text_field :tank_ref, class: 'form-control' %>
  </div>
  <div class="form-group hidden-field">
    <label class="head-small">Name</label>
    <%= f.text_field :name, class: 'form-control' %>
  </div>
  <div class="form-group hidden-field">
    <label class="head-small">Linked Overflow Ref.</label>
    <%= f.text_field :overflow_ref, class: 'form-control' %>
  </div>
  <div class="form-group hidden-field">
    <label class="head-small">Online/Offline</label>
    <%= f.text_field :online, class: 'form-control' %>
  </div>
  <div class="form-group hidden-field">
    <label class="head-small">Permitted Storage Volume (m3)</label>
    <%= f.text_field :permitted_storage, class: 'form-control' %>
  </div>
  <div class="form-group hidden-field">
    <label class="head-small">Effective Storage Volume (m3)</label>
    <%= f.text_field :effective_storage, class: 'form-control' %>
  </div>
  <div class="form-group hidden-field">
    <label class="head-small">Annual Volume To Tank (m3)</label>
    <%= f.text_field :annual_volume, class: 'form-control' %>
  </div>
  <div class="form-group hidden-field">
    <label class="head-small">Comments</label>
    <%= f.text_field :comments, class: 'form-control' %>
  </div>
  <div class="form-group hidden-field">
    <%= link_to_remove_association 'Delete Row', f, class: "delete-row" %>
  </div>
</div></br>
